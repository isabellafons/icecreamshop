<div class="row">
  <div class="col s8">
    <div class="card">    
      <div class="card-content">
        <span class="card-title"><%= type_of_form %></span>
        <%= simple_form_for(object) do |f| %>
          <%= f.error_notification %>
          <div class="row">
            <div class="form-inputs">
              <%= render partial: "#{fields}", locals: { f: f } %>
          </div>
        </div>
      </div>
    </div>

      <div class="row">
        <div class="form-actions col s8">
          <%= f.button :submit %>
        </div>
      
    <% end %>
   
    
  </div>
  <%= link_to 'Back', :back %>
</div>



<div class = "col s4">
    <%= render partial: "#{sidebar}" %>
    </div>
    
  